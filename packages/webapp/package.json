  "scripts": {
    "postinstall": "npm run build",
    "local:install": "bin/init-webapp-env-if-needed.sh",
    "local:start": "npm run local:install && . ../../docker/secrets/WEBAPP_ENV && npm run start:dev",
    "build": "bin/build.sh",
    "build:clean": "bin/build-clean-public.sh",
    "build:copy-static": "bin/build-copy-static.sh",
    "build:css": "bin/build-css.sh",
    "build:js": "bin/build-js.sh",
    "start": "node ./src/index.js",
    "start:dev": "nodemon -e js,html ./src/index.js",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "lint": "standard",
    "docker:build": "docker build . -t webapp",
    "docker:start": "docker run --name webapp -p 3000:3000 -d webapp",
    "docker:stop": "docker kill webapp && docker rm webapp"
